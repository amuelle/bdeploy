{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./ui/webapp",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./ui/webapp/dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2020", "dom"],
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  }
}
